<p id="notice"><%= notice %></p>

<h1>Libraries</h1>

<div class="list-group">
      <% @libraries.each do |library| %>
      <a href="<%= url_for(library_path(library)) %>" class="list-group-item" %>" <%= library.name %> <span class="badge"><%= library.size %></a>
      <% end %>
</div>

<br>

<%= link_to 'New Library', new_library_path %>
